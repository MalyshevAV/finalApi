{
  "$schema": "http://json-schema.org/draft-04/schema#",

    "type": "object",
    "properties": {
      "guid": {
        "type": "string",
        "minLength": 36,
        "maxLength": 36
      },
      "archive": {
        "type": "boolean"
      },
      "dateOutputArchive": {
        "type": "string",
        "minLength": 10,
        "maxLength": 20
      },
      "dateFrom": {
        "type": "string",
        "minLength": 10,
        "maxLength": 20
      },
      "dateBy": {
        "type": "string",
        "minLength": 10,
        "maxLength": 20
      },

      "code": {
        "type": "string",
        "minLength": 0,
        "maxLength": 9
      },
      "name": {
        "type": "string",
        "minLength": 1,
        "maxLength": 150
      }
    },
    "required": [
      "guid",
      "archive",
      "code",
      "name",
      "dateFrom",
      "dateBy"
    ]
  }
