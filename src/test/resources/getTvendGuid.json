{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type" : "object",
    "properties" : {
      "guid": {
        "type": "string",
        "minLength": 36,
        "maxLength": 36
      },
      "code": {
        "type": "string",
        "minLength": 0,
        "maxLength": 10
      },
      "name": {
        "type": "string",
        "minLength": 0,
        "maxLength": 150
      },
      "unit": {
        "type": "string",
        "minLength": 36,
        "maxLength": 36
      },
      "commodity": {
        "type": "boolean"
      },
      "traceableItem": {
        "type": "boolean"
    },
       "archive": {
       "type": "boolean"
    },
      "dateOutputArchive": {
        "type": ["null","string"],
        "minLength": 10,
        "maxLength": 20
      }
    },
      "required": ["guid","code", "name", "unit","commodity", "traceableItem", "archive", "dateOutputArchive" ]
    }

