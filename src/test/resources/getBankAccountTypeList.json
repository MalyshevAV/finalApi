{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "guid": {
        "type": "string",
        "minLength": 36,
        "maxLength": 36
      },
      "name": {
        "type": "string",
        "minLength": 1,
        "maxLength": 100
      },
      "code": {
        "type": "string",
        "minLength": 1,
        "maxLength": 20
      }
    },
    "required": ["guid","name", "code" ]
  }
}